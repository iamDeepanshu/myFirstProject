<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>query</servlet-name>
        <servlet-class>com.MyProject.QueryDatabaseServlet</servlet-class>
    </servlet>
        <servlet-mapping>
            <servlet-name>query</servlet-name>
            <url-pattern>/query</url-pattern>
        </servlet-mapping>



    <servlet>
        <servlet-name>signup</servlet-name>
        <servlet-class>com.MyProject.Signup</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signup</servlet-name>
        <url-pattern>/signup1</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>signin</servlet-name>
        <servlet-class>com.MyProject.SignIn</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signin</servlet-name>
        <url-pattern>/signin1</url-pattern>
    </servlet-mapping>


</web-app>